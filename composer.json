{
	"name": "b13/bolt",
	"type": "typo3-cms-extension",
	"description": "Bolt - An easy TYPO3 integration basis",
	"homepage": "https://github.com/b13/bolt",
	"license": "GPL-2.0-or-later",
	"keywords": ["TYPO3 CMS", "Integrators", "Site Extension", "Site Package", "TypoScript"],
	"require": {
		"typo3/cms-core": "^10.4"
	},
	"replace": {
		"cmsexperts/bolt": "*",
		"typo3-ter/bolt": "*"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "bolt"
		}
	},
	"autoload": {
		"psr-4": {
			"B13\\Bolt\\": "Classes"
		}
	}
}
